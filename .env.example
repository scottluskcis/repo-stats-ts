ORG_NAME=your_org

# octokit 
BASE_URL=https://api.github.com
PROXY_URL=
PAGE_SIZE=10
EXTRA_PAGE_SIZE=50

# logger
VERBOSE=false

# auth 
ACCESS_TOKEN=your_token_here
APP_ID=
PRIVATE_KEY=
PRIVATE_KEY_FILE=
APP_INSTALLATION_ID=

# rate limit check 
RATE_LIMIT_CHECK_INTERVAL=25

# retry - exponential backoff
RETRY_MAX_ATTEMPTS=3
RETRY_INITIAL_DELAY=1000
RETRY_MAX_DELAY=30000
RETRY_BACKOFF_FACTOR=2
RETRY_SUCCESS_THRESHOLD=5

# resume option
RESUME_FROM_LAST_SAVE=false

# repository list
REPO_LIST=

# auto-process missing repositories
AUTO_PROCESS_MISSING=false
